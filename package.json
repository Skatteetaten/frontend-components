{
  "name": "@skatteetaten/frontend-components",
  "groupId": "no.skatteetaten.aurora",
  "license": "Apache-2.0",
  "version": "4.0.0",
  "private": false,
  "main": "lib/index.js",
  "bin": {
    "skatteetaten-frontend-components-import-maps": "./lib/bin/importMaps/writeImportMaps.js"
  },
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Skatteetaten/frontend-components.git"
  },
  "files": [
    "docs",
    "lib",
    "metadata"
  ],
  "publishConfig": {
    "registry": "https://nexus.sits.no/repository/npm-internal/"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <17.0.0",
    "react-dom": ">=16.8.0 <17.0.0"
  },
  "dependencies": {
    "@fluentui/react": "7.153.2",
    "@uifabric/merge-styles": "7.19.1",
    "@uifabric/styling": "7.16.18",
    "@uifabric/utilities": "7.33.2",
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "hotkeys-js": "3.8.1",
    "i18next": "20.3.1",
    "material-design-icons": "3.0.1",
    "moment": "2.29.1",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react-i18next": "11.7.4",
    "react-transition-group": "4.4.1",
    "tslib": "2.0.3",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "7.13.10",
    "@ctrl/tinycolor": "3.2.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-image": "2.0.6",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-typescript": "8.2.1",
    "@rollup/plugin-url": "6.0.0",
    "@skatteetaten/ci-scripts": "2.0.5",
    "@svgr/rollup": "5.5.0",
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.10",
    "@types/react": "16.14.2",
    "@types/react-dom": "16.9.10",
    "@types/react-router": "5.1.8",
    "@types/react-transition-group": "4.4.0",
    "acorn": "6.4.2",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "es6-promise": "4.2.8",
    "eslint-plugin-prettier": "3.1.4",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "jest-junit": "12.0.0",
    "lodash.find": "4.6.0",
    "madge": "4.0.2",
    "node-fetch": "2.6.1",
    "normalizr": "3.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "16.14.0",
    "react-docgen-typescript": "1.20.1",
    "react-dom": "16.14.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-styleguidist": "11.1.4",
    "react-test-renderer": "16.14.0",
    "rimraf": "3.0.2",
    "rollup": "2.45.1",
    "rollup-plugin-bundle-size": "1.0.3",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-node-externals": "2.2.0",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-terser": "7.0.2",
    "sonar-scanner": "3.1.0",
    "typescript": "3.9.7",
    "webfont": "9.0.0"
  },
  "scripts": {
    "start": "REACT_APP_BUILD_VERSION=$(git rev-parse --abbrev-ref HEAD) FAST_REFRESH=false styleguidist server --verbose",
    "generate:icons": "webfont --normalize --fontHeight 600 src/webfont/**/*.svg -d src/components/utils/assets",
    "prebuild": "run-s clean generate:icons circularDeps lint",
    "build": "run-p build:lib build:docs",
    "build:docs": "styleguidist build",
    "build:lib": "run-p build:types build:umds build:commonjs build:assets",
    "build:assets": "cpx \"./src/components/**/assets/**/*\" ./lib",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:copy-files-external": "node scripts/copyBuildFiles.js EXTERNAL",
    "build:commonjs": "babel ./src/components --out-dir lib --extensions '.ts,.tsx' ",
    "build:umd-warnings": "rollup -c -m",
    "build:umds": "run-s build:umd build:umd-prod build:importmaps",
    "build:umd": "rollup --silent -c -m || exit 1",
    "build:umd-prod": "rollup --silent -c -m --mini --prod || exit 1",
    "build:importmaps": "mkdir -p lib/bin/importmaps || true && cp -R importmaps/* lib/bin/importmaps && node lib/bin/importmaps/writeImportMaps.js lib ../../../package.json",
    "build:types": "tsc --build tsconfig.types.json",
    "lint": "run-s -l lint:prettier lint:eslint",
    "lint:ci": "eslint src/**/*.{js,jsx} -f checkstyle -o target/checkstyle-result.xml",
    "lint:fix": "run-s -l lint:prettier-fix lint:eslint-fix",
    "lint:eslint": "eslint 'src/**/*{.ts,.tsx}'",
    "lint:prettier": "prettier --check src/**/*{.ts,.tsx,.js,.jsx,.css,.md}",
    "lint:eslint-fix": "eslint --fix 'src/**/*{.ts,.tsx}'",
    "lint:prettier-fix": "prettier --loglevel warn --write src/**/*{.ts,.tsx,.js,.jsx,.css,.md}",
    "sonarqube": "sonar-scanner",
    "test": "react-scripts test --env=jsdom --verbose=false",
    "test:coverage": "CI=true react-scripts test --env=jsdom --coverage --testResultsProcessor jest-junit",
    "circularDeps": "node scripts/checkCircularDeps.js",
    "postversion": "npm run build:copy-files",
    "clean": "rimraf coverage lib docs *.tgz",
    "deploy:gh-pages": "gh-pages -d docs -p 0 -r https://github.com/Skatteetaten/frontend-components.git",
    "eject": "react-scripts eject"
  },
  "engines": {
    "npm": ">=6.0.0"
  },
  "webfont": {
    "fontName": "SkeIconfont",
    "normalize": true,
    "round": false,
    "centerHorizontally": true,
    "template": "src/webfont/icons-ske.ts.njk",
    "templateFontPath": "../assets/",
    "destTemplate": "src/components/utils/icons"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.js",
      "!src/setupTests.js",
      "!src/styleguide/**",
      "!src/components/utils/icons/**",
      "!src/components/utils/assets/**"
    ]
  },
  "jest-junit": {
    "suiteName": "Unit Tests",
    "outputDirectory": "coverage/surefire-reports",
    "outputName": "TEST-jest.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "auto",
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "no-unused-vars": "off",
      "import/no-anonymous-default-export": "off",
      "sort-imports": [
        "warn",
        {
          "ignoreCase": true,
          "ignoreDeclarationSort": true,
          "ignoreMemberSort": true,
          "memberSyntaxSortOrder": [
            "all",
            "single",
            "multiple",
            "none"
          ]
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
